
<h2>Sign up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<table>
<tr>
<td><%= f.label :first_name %></td>
<td><%= f.text_field :first_name %></td>
</tr>

<tr>
<td><%= f.label :last_name %></td>
<td> <%= f.text_field :last_name %></td>
</tr>

<tr>
<td><%= f.label :school_name,"College Name" %></td>
<td><%= f.text_field :school_name %></td>
</tr>

<tr>
<td><%= f.label :email %></td>
<td><%= f.email_field :email %></td>
</tr>

<tr>
<td><%= f.label :password %></td>
<td> <%= f.password_field :password %></td>
</tr>

<tr>
<td><%= f.label :password_confirmation,"confirm Password" %></td>
<td><%= f.password_field :password_confirmation %></td>
</tr>
<tr>
<td><%= f.label :MobileNo %></td>
<td><%= f.text_field :mobile_no %></td>
</tr>
<tr>
<td><%= f.label :Programing_language,"Select Language" %></td>
<td> <%=f.collection_select :subject_id, Subject.all , :id, :subject, :include_blank => false  %></td>
</tr>
<tr>
  <td colspan="2" ><%= f.submit "Sign up",class: "submit_button"%></td>
</tr>
</table>

<% end %>

<%= render "devise/shared/links" %>
