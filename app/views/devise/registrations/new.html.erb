<div class="content_container">
<h2>Register</h2>

<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%#= devise_error_messages! %>
  <%#= @user.errors.full_messages.first if @user.errors.any? %>
<table>
<tr>
<td><%= f.input :first_name, :autofocus => true,placeholder: 'First Name' %></td>
</tr>

<tr>
<td><%= f.input :last_name, :autofocus => true,placeholder: 'Last Name' %></td>
</tr>

<tr>
<td><%= f.input :school_name, :autofocus => true,placeholder: 'College Name' %></td>
</tr>

<tr>
<td><%= f.input :email, :autofocus => true,placeholder: 'Email' %></td>
</tr>

<tr>
<td><%= f.input :password,placeholder: 'Password' %></td>
</tr>

<tr>
<td><%= f.input :password_confirmation,placeholder: 'Confirm_Password' %></td>
</tr>
<tr>
<td><%= f.input :mobile_no, :autofocus => true,placeholder: 'Mobile number' %></td>
</tr>
<tr>
<td><%= f.input :subject_id, collection: Subject.all(order: 'subject'), prompt: "Choose a Subject" %></td>
</tr>

  </table>
  <div id="reset">
<%= f.submit "Register",class: "actual_logbtn"%>
  <%= f.submit "Reset", :type => "reset",class: "actual_logbtn" %>
</div>
<% end %>
</div>
<%#= render "devise/shared/links" %>
