<div class="content_container">
<h2>Register</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<table>
<tr>
<td><%= f.label :first_name %></td>
<td><%= f.text_field :first_name,:autofocus => true %></td>
</tr>

<tr>
<td><%= f.label :last_name %></td>
<td> <%= f.text_field :last_name,:autofocus => true %></td>
</tr>

<tr>
<td><%= f.label :school_name,"College Name" %></td>
<td><%= f.text_field :school_name,:autofocus => true %></td>
</tr>

<tr>
<td><%= f.label :email %></td>
<td><%= f.email_field :email,:autofocus => true %></td>
</tr>

<tr>
<td><%= f.label :password %></td>
<td> <%= f.password_field :password,:autofocus => true %></td>
</tr>

<tr>
<td><%= f.label :password_confirmation,"confirm Password" %></td>
<td><%= f.password_field :password_confirmation,:autofocus => true %></td>
</tr>
<tr>
<td><%= f.label :MobileNo,"Mobile Number" %></td>
<td><%= f.text_field :mobile_no,:autofocus => true %></td>
</tr>
<tr>
<td><%= f.label :Programing_language,"Select Language" %></td>
<td> <%=f.collection_select :subject_id, Subject.all , :id, :subject, :include_blank => false ,:autofocus => true %></td>
</tr>
<tr>
  <td colspan="2" style="text-align:center;"><%= f.submit "Start Exam",class: "actual_logbtn"%></td>
</tr>
</table>

<% end %>
</div>
<%#= render "devise/shared/links" %>
